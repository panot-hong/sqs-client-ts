{
  "name": "sqs-sample",
  "version": "1.0.0",
  "description": "demonstrate how to work with SQS with nodejs",
  "main": "app.js",
  "scripts": {
    "start:local:dependencies": "docker-compose -f ./docker/docker-compose.yml up",
    "create:local:queue": "aws sqs create-queue --queue-name digested-content-queue --endpoint-url http://localhost:4576",
    "test": "export AWS_ACCESS_KEY_ID=foo && AWS_ACCOUNT_ID=066223515768 && npm run compile && mocha --require ts-node/register test/**/*.spec.ts",
    "compile": "tsc"
  },
  "keywords": [
    "sqs",
    "nodejs"
  ],
  "author": "Hong",
  "license": "ISC",
  "dependencies": {
    "@types/uuid": "^3.4.5",
    "aws-sdk": "^2.517.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.2",
    "chai": "^4.2.0",
    "mocha": "^6.2.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  }
}
